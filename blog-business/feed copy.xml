---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <link href="{{- site.url -}}{{- site.baseurl -}}/blog-business/feed.xml" rel="self" type="application/atom+xml"/>
    <link href="{{- site.url -}}{{- site.baseurl -}}/blog-business/" rel="alternate" type="text/html"/>
    <updated>{{- site.time | date_to_xmlschema-}}</updated>
    <id>{{- site.url -}}{{- site.baseurl -}}/blog-business/feed.xml</id>
    <title>{{- site.title -}} Business Blog</title>
    <subtitle> {{- site.description | strip_html | lstrip | rstrip -}}</subtitle>
    <author>
        <name>
            {%- if site.author -%}
                {{- site.author -}}
            {%- else -%}
                {{- site.title -}}
            {%- endif -%}
        </name>
    </author>

    {% for post in site.categories.business limit:20 %}
    <entry>
        <title> {{- post.title -}} </title>
        <pubDate>{{ post.date | date_to_schema}}</pubDate>
        <id>{{- site.url -}}{{- site.baseurl -}}{{- post.url -}}</id>
        <link>{{ site.url }}{{- site.baseurl -}}{{ post.url }}</link>
        <updated>{{ post.date | date_to_schema}}</updated>
        <summary>
            {% if post.excerpt %}
            {{ post.excerpt | strip_html | lstrip | rstrip }}
            {% endif %}
        </summary>
    </entry>
    {%- endfor -%}

</feed>